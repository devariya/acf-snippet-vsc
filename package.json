{
	"name": "ACF-Snippet",
	"publisher": "anthonydiametrix",
	"description": "Advanced Custom Field Snippets",
	"icon": "images/logo.jpg",
	"version": "1.9.0",
	"engines": {
		"vscode": "0.10.x"
	},
	"scripts": {
		"build": "node ./scripts/build.js"
	},
	"categories": [
		"Snippets"
	],
	"keywords": [
		"acf",
		"Advanced Custom Fields",
		"WordPress"
	],
	"homepage": "https://github.com/hubsta/acf-snippet-vsc/blob/master/README.md",
	"repository": {
		"type": "git",
		"url": "https://github.com/hubsta/acf-snippet-vsc.git"
	},
	"contributes": {
		"languages": [
			{
				"id": "blade",
				"aliases": [
					"Laravel Blade",
					"Blade",
					"blade"
				],
				"extensions": [
					".blade.php"
				],
				"configuration": "./blade.configuration.json"
			}
		],
		"snippets": [
			{
				"language": "php",
				"path": "./snippets/php-html.json"
			},
			{
				"language": "html",
				"path": "./snippets/php-html.json"
			},
			{
				"language": "blade",
				"path": "./snippets/blade.json"
			}
		]
	}
}